}
.login-logo{ display: block; width: 30%; height: 15%; margin: 25% auto 0;}
.bei-logo ._h2{ font-size: 20px; color: #1f3f59; position: absolute; z-index: 2; top: 160px; text-align: center; width: 100%; font-family: "\5FAE\8F6F\96C5\9ED1";}
.login{ background: #FFFFFF; position: absolute; z-index: 1; width: 100%; left:0%;top:210px; padding-top: 50px;}
.mui-input-line{ width: 80%; margin:0 10%; float: left; position: relative;}
.mui-input-line ._input{ position: relative;z-index: 2; height: 39px; width: 70% !important;}
.mui-input-line:after{
	   content: "";
    position: absolute;
    top: -50%;
    bottom: -50%;
    left: -50%;
    right: -50%;
    -webkit-transform: scale(0.5);
    -ms-transform: scale(0.5);
        transform: scale(0.5);
    border-bottom: solid 1px #c6c6c8;
    z-index: 1;
}
.Otherlogin{ width: 100%; position: absolute; bottom: 0; left: 0;}
.Otherlogin-line{ width:35%;position: relative; height: 10px; float: left;}
.Otherlogin-line:after{
	   content: "";
    position: absolute;
    top: -50%;
    bottom: -50%;
    left: -50%;
    right: -50%;
    -webkit-transform: scale(0.5);
    -ms-transform: scale(0.5);
        transform: scale(0.5);
    border-bottom: solid 1px #c6c6c8;
    z-index: 1;
}
